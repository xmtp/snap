{
  "name": "@xmtp/snap",
  "version": "1.3.4",
  "description": "Snap keystore implementation for XMTP",
  "repository": {
    "type": "git",
    "url": "https://github.com/xmtp/snap.git"
  },
  "license": "MIT",
  "main": "dist/bundle.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "yarn clean:lib && mm-snap build --transpilationMode=localAndDeps",
    "clean": "rm -rf .turbo && rm -rf node_modules && yarn clean:lib",
    "clean:lib": "rm -rf dist",
    "dev": "yarn clean:lib && mm-snap watch --transpilationMode=localAndDeps",
    "eval": "mm-snap eval",
    "format": "yarn format:base -w .",
    "format:base": "prettier --ignore-path ../../.gitignore",
    "format:check": "yarn format:base -c .",
    "lint": "eslint . --ignore-path ../../.gitignore",
    "serve": "mm-snap serve",
    "test": "yarn build && jest --runInBand"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "dependencies": {
    "@xmtp/proto": "3.34.0",
    "@xmtp/xmtp-js": "11.3.9",
    "async-mutex": "^0.4.0",
    "buffer": "^6.0.3",
    "protobufjs": "^7.2.4"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.0.3",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@metamask/snaps-cli": "^4.0.0",
    "@metamask/snaps-jest": "^4.0.1",
    "@metamask/snaps-sdk": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsdoc": "^43.0.7",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^6.6.2",
    "jest": "^29.6.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
