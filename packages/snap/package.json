{
  "name": "@xmtp/snap",
  "version": "1.3.6",
  "description": "Snap keystore implementation for XMTP",
  "repository": {
    "type": "git",
    "url": "https://github.com/xmtp/snap.git"
  },
  "license": "MIT",
  "main": "dist/bundle.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "yarn clean:lib && mm-snap build --transpilationMode=localAndDeps",
    "clean": "rm -rf .turbo && rm -rf node_modules && yarn clean:lib",
    "clean:lib": "rm -rf dist",
    "dev": "yarn clean:lib && mm-snap watch --transpilationMode=localAndDeps",
    "eval": "mm-snap eval",
    "format": "yarn format:base -w .",
    "format:base": "prettier --ignore-path ../../.gitignore",
    "format:check": "yarn format:base -c .",
    "lint": "eslint . --ignore-path ../../.gitignore",
    "serve": "mm-snap serve",
    "test": "yarn build && jest --runInBand",
    "typecheck": "tsc"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "dependencies": {
    "@xmtp/proto": "3.62.1",
    "@xmtp/xmtp-js": "12.1.0",
    "async-mutex": "^0.5.0",
    "buffer": "^6.0.3",
    "protobufjs": "^7.3.2"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^3.1.0",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@metamask/snaps-cli": "^6.2.1",
    "@metamask/snaps-jest": "^8.2.0",
    "@metamask/snaps-sdk": "^6.1.1",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsdoc": "^48.9.2",
    "eslint-plugin-n": "^17.10.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.0.0",
    "ethers": "^6.13.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
